{
  "$schema": "../../node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:8080/api/swagger-json",
  "output": "./src/api/generated",
  "templates": "./src/api/templates",
  "configuration": "ApiConfiguration",
  "module": "ApiModule",
  "baseService": "ApiBaseService",
  "serviceSuffix": "ApiService",
  "requestBuilder": "ApiRequestBuilder",
  "response": "ApiStrictHttpResponse",
  "enumStyle": "pascal",
  "ignoreUnusedModels": true,
  "excludeTags": ["App"]
}
